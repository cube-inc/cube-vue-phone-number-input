{
  "name": "cube-vue-phone-number-input",
  "version": "0.3.0",
  "private": false,
  "description": "A super simple phone number input component for Vue based on libphonenumber-js.",
  "author": "Fred Gomez <fred@cube.rentals>",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "bundle": "rollup --config rollup.config.js",
    "prepublishOnly": "yarn lint && yarn test:unit && yarn bundle",
    "watch": "yarn bundle --watch"
  },
  "main": "dist/cube-vue-phone-number-input.umd.js",
  "module": "dist/cube-vue-phone-number-input.esm.js",
  "browser": {
    "./sfc": "src/components/InputPhoneNumber.vue"
  },
  "unpkg": "dist/cube-vue-phone-number-input.min.js",
  "files": [
    "dist",
    "src/assets",
    "src/scss",
    "src/components/InputPhoneNumber.vue"
  ],
  "dependencies": {
    "core-js": "^3.6.4",
    "libphonenumber-js": "^1.7.15"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^3.0.0",
    "@rollup/plugin-json": "^4.0.1",
    "@vue/cli-plugin-babel": "~4.2.2",
    "@vue/cli-plugin-eslint": "~4.2.2",
    "@vue/cli-plugin-unit-jest": "^3.6.0",
    "@vue/cli-service": "~4.2.2",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "core-js": "^2.6.5",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^4.9.0",
    "rollup": "^1.29.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-progress": "^1.1.1",
    "rollup-plugin-terser": "^5.1.3",
    "rollup-plugin-vue": "^5.1.5",
    "sass-loader": "^7.1.0",
    "vue": "^2.6.10",
    "vue-template-compiler": "^2.5.21"
  },
  "keywords": [
    "vue",
    "phone number",
    "input"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/cube-inc/cube-vue-phone-number-input.git"
  }
